{
  "name": "recipes",
  "version": "1.0.0",
  "description": "Vegetarian Recipes",
  "main": "index.js",
  "scripts": {
    "prelint": "npm install",
    "lint": "find src -name '*.ts' | xargs node_modules/tslint/bin/tslint",
    "precreateReadme": "npm run lint",
    "createReadme": "echo '##[Meat Temperature](https://rawgit.com/clickthisnick/recipes/master/meatTemp.html) \\n\\n # Recipes \\n\\n' > README.MD",
    "preremoveDist": "npm run createReadme",
    "removeDist": "./src/builder/removeDist.sh",
    "prebuild": "npm run removeDist",
    "build": "./node_modules/ts-node/dist/bin.js ./src/builder/createDist.ts",
    "postBuild": "./node_modules/ts-node/dist/bin.js ./src/makeReadme.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clickthisnick/recipes.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/clickthisnick/recipes/issues"
  },
  "homepage": "https://github.com/clickthisnick/recipes#readme",
  "dependencies": {
    "@types/node": "^10.3.3",
    "ts-node": "^6.1.1",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  }
}
