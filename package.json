{
  "name": "recipes",
  "version": "1.0.0",
  "description": "Vegetarian Recipes",
  "main": "index.js",
  "scripts": {
    "lint": "find src -name '*.ts' | xargs node_modules/tslint/bin/tslint",
    "compile": "./node_modules/typescript/bin/tsc",
    "createReadme": "echo '# Recipes \\n\\n' > README.MD",
    "preremoveJs": "npm run createReadme",
    "removeJs": "./src/builder/removeDist.sh",
    "prebuild": "npm run removeJs",
    "build": "find src/recipes/ -iname '*.ts' -exec sh -c 'node_modules/ts-node/dist/bin.js {}' \\;"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clickthisnick/recipes.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/clickthisnick/recipes/issues"
  },
  "homepage": "https://github.com/clickthisnick/recipes#readme",
  "dependencies": {
    "@types/node": "^8.5.9",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  }
}
